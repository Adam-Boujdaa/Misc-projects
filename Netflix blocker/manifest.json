{
  "manifest_version" : 3,
  "name": "Netflix, Amazon video, crunchyroll, disneyplus, twitch  blocker",
  "version": "1",
  "description" : "A basic custom made Netflix blocker that works how adblockers work",
  "permissions": [  "webRequest",
  "webRequestBlocking",
  "declarativeNetRequest", 
  "declarativeNetRequestWithHostAccess" , 
  "declarativeNetRequestFeedback"  
  ],


  "host_permissions": [
    "*://*.netflix.com/*",
    "*://*.primevideo.com/*",
    "*://*.amazon.com/*",
    "*://*.crunchyroll.com/*",
    "*://*.disneyplus.com/*",
    "*://*.twitch.tv/*"
  ],


  "background" : {
    "background.service_worker" : ["background.js"]
  },
  
  "icons" : {
    "128" : "defaulticon.png",
    "64" : "defaulticon.png"
  }
}
